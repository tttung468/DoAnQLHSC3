<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class catalog="qlhsc3DB"
		name="com.toanhuuvuong.model.Performance" optimistic-lock="version"
		table="performance">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="code" type="string">
			<column name="code" unique="true" />
		</property>
		<property name="name" type="string">
			<column name="name" />
		</property>
		<property name="lowerBound" type="java.lang.Float">
			<column name="lowerbound" precision="12" scale="0" />
		</property>
		<property name="upperBound" type="java.lang.Float">
			<column name="upperbound" precision="12" scale="0" />
		</property>
		<property name="control" type="java.lang.Float">
			<column name="control" precision="12" scale="0" />
		</property>
		<property name="isDeleted" type="java.lang.Boolean">
			<column name="isdeleted" />
		</property>
		<property name="createdDate" type="timestamp">
			<column length="19" name="createddate" not-null="true" />
		</property>
		<property name="createdBy" type="string">
			<column name="createdby" />
		</property>
		<property name="modifiedDate" type="timestamp">
			<column length="19" name="modifieddate" />
		</property>
		<property name="modifiedBy" type="string">
			<column name="modifiedby" />
		</property>
	</class>
</hibernate-mapping>
